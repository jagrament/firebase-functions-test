{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
        "source": "/sdk_v4/**", "function": "sdkV4-main"
      },{
        "source": "/console_v4/**", "function": "console_v4-main"
      },{
        "source": "/mint_admin_v4/**", "function": "mint_admin_v4-main"
      },{
        "source": "/background_functions_v4_on_write_ipfs_datas/**", "function": "background_functions_v4_on_write_ipfs_datas-main"
      },{
        "source": "/background_functions_v4_on_every_hour/**", "function": "background_functions_v4_on_every_hour-main"
      },{
        "source": "/background_functions_v4_on_every_five_mimutes/**", "function": "background_functions_v4_on_every_five_mimutes-main"
      },{
        "source": "/background_functions_v4_on_every_one_mimutes/**", "function": "background_functions_v4_on_every_one_mimutes-main"
      },{
        "source": "/admin_v4/**", "function": "admin_v4-main"
      },{
        "source": "/webhook_v4/**", "function": "webhook_v4-main"
      }
    ]
  }
}
